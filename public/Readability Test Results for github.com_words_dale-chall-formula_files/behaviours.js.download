$(function(){
    $('#tabs ul a').bind('click', function (event) {
        event.preventDefault()
        var $this = $(event.target);
        console.log($this)

        $this.parent().addClass('ui-state-active').siblings().removeClass('ui-state-active');
        $($this.attr('href')).addClass('ui-state-active').siblings().removeClass('ui-state-active');

        $('.js-selected-tab').val($this.attr('data-tab'))
    })

	$("#accordion").accordion({ header: "h3" });
	//$('#tabs').tabs();



});